{
	"name": "static-site-boilerplate",
	"private": true,
	"description": "A simple starter to get up and developing quickly with Gatsby and TS",
	"version": "0.1.0",
	"author": "Martin Sweeny <martin@wiseweb.solutions>",
	"engines": {
		"node": "11"
	},
	"dependencies": {
		"@emotion/core": "^10.0.14",
		"@emotion/styled": "^10.0.14",
		"@types/react-responsive": "^3.0.3",
		"dotenv": "^8.0.0",
		"gatsby": "^2.10.4",
		"gatsby-image": "^2.2.3",
		"gatsby-plugin-alias-imports": "^1.0.5",
		"gatsby-plugin-emotion": "^4.1.0",
		"gatsby-plugin-manifest": "^2.2.0",
		"gatsby-plugin-offline": "^2.2.0",
		"gatsby-plugin-react-helmet": "^3.1.0",
		"gatsby-plugin-react-svg": "^2.1.2",
		"gatsby-plugin-sharp": "^2.2.1",
		"gatsby-plugin-typescript": "^2.1.2",
		"gatsby-plugin-typescript-checker": "^1.1.1",
		"gatsby-source-filesystem": "^2.1.0",
		"gatsby-transformer-sharp": "^2.2.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-responsive": "^8.0.1",
		"typescript": "^3.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@commitlint/cli": "^8.0.0",
		"@commitlint/config-conventional": "^8.0.0",
		"@types/enzyme": "^3.10.3",
		"@types/jest": "^24.0.15",
		"@types/react-helmet": "^5.0.8",
		"@types/react-test-renderer": "^16.9.0",
		"@typescript-eslint/eslint-plugin": "^1.11.0",
		"@typescript-eslint/parser": "^1.11.0",
		"babel-jest": "^24.9.0",
		"babel-plugin-module-resolver": "^3.2.0",
		"babel-preset-gatsby": "^0.2.8",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"enzyme-to-json": "^3.4.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"husky": "^3.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.9.0",
		"jest-cli": "^24.9.0",
		"jest-emotion": "^10.0.14",
		"jest-junit": "^7.0.0",
		"plop": "^2.4.0",
		"prettier": "^1.18.2",
		"react-test-renderer": "^16.9.0",
		"ts-jest": "^24.0.2",
		"ts-node": "^8.3.0"
	},
	"keywords": [],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop --host 0.0.0.0",
		"lint:report": "eslint --ext .tsx,.ts --format html --output-file ./eslint-report.html ./src",
		"lint": "eslint --ext .tsx,.ts ./src",
		"format": "prettier --write src/**/*.{js,jsx}",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"test": "jest",
		"test:ci": "./node_modules/.bin/jest  --ci --coverage --reporters=default --reporters=jest-junit --silent",
		"test:coverage": "jest --coverage",
		"types:check": "./node_modules/.bin/tsc --pretty"
	},
	"repository": {
		"type": "git",
		"url": "https://git.appserver.wiseweb.dev/martin.sweeny/static-site-boilerplate"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn test",
			"pre-push": "yarn test",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	}
}
